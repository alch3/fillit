t_list ft_lstnew(int i, const char *str)
{
	t_list  *newelement;

	newelement = (t_list*)malloc(sizeof(t_list));
	if (!newelement)
		ft_putstr("ERROR malloc\n");
	newelement->index = i;
	newelement->tetrimino = ft_strdup(str);
	newelement->next = NULL;
}










